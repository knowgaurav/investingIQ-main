# InvestingIQ Backend Environment Variables

# Database
DATABASE_URL=postgresql://postgres:postgres@localhost:5432/investingiq

# Azure Service Bus (use emulator for local development)
# Local emulator connection string:
AZURE_SERVICEBUS_CONNECTION_STRING=Endpoint=sb://localhost;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=SAS_KEY_VALUE;UseDevelopmentEmulator=true;

# For production, use your Azure Service Bus connection string:
# AZURE_SERVICEBUS_CONNECTION_STRING=Endpoint=sb://your-namespace.servicebus.windows.net/;SharedAccessKeyName=RootManageSharedAccessKey;SharedAccessKey=your-key

# LLM (OhMyGPT)
OHMYGPT_API_KEY=your-ohmygpt-api-key
OHMYGPT_API_BASE=https://api.ohmygpt.com/v1
LLM_MODEL=gemini-3-flash-preview

# News API
NEWS_API_KEY=your-newsapi-key

# MLflow
MLFLOW_TRACKING_URI=http://localhost:5000
MLFLOW_EXPERIMENT_NAME=investingiq-llm

# Rate Limiting
RATE_LIMIT=100/minute

# Debug
DEBUG=false
